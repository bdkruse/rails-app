<div>
  <h5><%= link_to 'Sign up', new_user_registration_path %></h1>
  <h5><%= link_to 'Sign in', new_user_session_path %></h1>
  <h5><%= link_to 'Log Out', destroy_user_session_path, method: :delete %></h1>
  <% if current_user.present? %>
    <div class="card">
      <div class="card-header">Current User</div>
      <div class="card-body">
        <p>id: <%= current_user.id %></p>
        <p>email: <%= current_user.email %></p>
      </div>
    </div>
  <% end %>
</div>